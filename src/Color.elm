module Color exposing (black, blue, dullRed, green, jetBlack, paleGray, red, white)

import Element exposing (rgb)


blue =
    rgb 0 0 0.8


white =
    rgb 1 1 1


paleGray =
    rgb 0.8 0.8 0.8


black =
    rgb 0.3 0.3 0.3


jetBlack =
    rgb 0.0 0.0 0.0


red =
    rgb 1.0 0.0 0.0


dullRed =
    rgb 0.5 0.0 0.0


green =
    rgb 0.0 1.0 0.0
